{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"strict": true,
		"baseUrl": "./src/lib/",
		"paths": {
			"supakit": ["types/index"],
			"supakit/vite": ["types/index"]
		}
	}
}

